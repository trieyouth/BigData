  <script language="javascript" type="text/javascript">
  function ClickGenerateData(){
        document.frm.action="<c:url value="/icpas/index/dataCollect/generateData"/>";
        document.frm.submit();
   }
  function ClickSaveForTemplate(){
      document.frm.action="<c:url value="/icpas/index/dataCollect/saveAsTemplate"/>";
