function system1meau1backgroundchange(){
	var bcchange1 = document.getElementById("system1-meau1");
	bcchange1.className = "c1"
	var bcchange2 = document.getElementById("system1-meau2");
	bcchange2.className = "c2"
	var bcchange3 = document.getElementById("system1-meau3");
	bcchange3.className = "c2"
	var bcchange4 = document.getElementById("system1-meau4");
	bcchange4.className = "c2"
	var bcchange5 = document.getElementById("system1-meau5");
	bcchange5.className = "c2"
	}
