// JavaScript Document
function datacollectshowmsg(){ 
   var msg = document.getElementById("datacollect");
   //alert("该显示提示");
   msg.style.visibility="visible";
}
function datacollecthiddenmsg(){
	 var msg = document.getElementById("datacollect");
     msg.style.visibility = "hidden";
	}
function datacollectclick(){
	window.location.href = "<c:url value="/ccas/index" />"
	}
	
	
function bishowshowmsg(){
   var msg = document.getElementById("bishow");
   //alert("该显示提示");
   msg.style.visibility="visible";
}
function bishowhiddenmsg(){
	 var msg = document.getElementById("bishow");
     msg.style.visibility = "hidden";
	}
function bishowclick(){
	window.location.href = "http://www.baidu.com"
	}
	
	

